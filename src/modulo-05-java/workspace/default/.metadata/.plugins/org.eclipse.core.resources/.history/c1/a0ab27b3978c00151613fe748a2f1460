package br.com.cwi.crescer;

public class DoubleLinkedList {

    // Classes internas
    private class Node {

        private String value;

        private Node next;

        public Node(String value, Node next) {
            this.value = value;
            this.next = next;
        }

        public Node(String value) {
            this.value = value;
        }

        public Node getNext() {
            return this.next;
        }

        public String getValue() {
            return this.value;
        }

        public void setNext(Node next) {
            this.next = next;
        }
    }

}
